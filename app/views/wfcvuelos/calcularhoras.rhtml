<%= button_to 'Imprimir PDF', { :action => 'genpdf', :tripulantes => @wfctripulantes }, :class => "btn btn-default" %>
<table class="table table-striped">
<% if (@errores != nil && @errores.length > 0) %>
  <tr>
    <td class="danger" colspan="9">Los siguientes logbooks presentan errores en la fecha y seran excluidos del calculo de horas</td>
  </tr>
  <% for error in @errores %>
    <tr>
      <th>#LOGBOOK</th>
      <th>Dia Inicio</th>
      <th>Mes Inicio</th>
      <th>AÃ±o Inicio</th>
      <th>Hora Inicio</th>
      <th>Min Inicio</th>
      <th>Origen</th>
      <th>Destino</th>
      <th>Link</th>
    </tr>
    <tr>
      <td><%=  error.logbook %> </td>
      <td><%=  error.diaini %> </td>
      <td><%=  error.mesini %> </td>
      <td><%=  error.anoini %> </td>
      <td><%=  error.horaini %> </td>
      <td><%=  error.minini %> </td>
      <td><%=  error.origen %> </td>
      <td><%=  error.destino %> </td>
      <td><%= link_to 'Editar', { :controller => 'wfcvuelos', :action => 'confirmar', :id => error.id }, :class => 'btn btn-default' %></td>
    </tr>
  <%end%>
</table>
<% end %>

<p><b>Reporte de horas de Vuelo</b></p>
<p> Desde: <%= ancer(@intdiaini) %>/<%= ancer(@intmesini) %>/<%= ancer(@intanoini) %> </p>
<p> Hasta: <%= ancer(@intdiafin) %>/<%= ancer(@intmesfin) %>/<%= ancer(@intanofin) %> </p>

